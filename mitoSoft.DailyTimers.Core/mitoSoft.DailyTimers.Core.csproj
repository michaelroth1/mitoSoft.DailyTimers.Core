<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>mitoSoft</Authors>
    <Description>A library for lightweight daily and weekly timer handling in C# </Description>
    <Copyright>(c)mitoSoft 2021</Copyright>
    <PackageProjectUrl>https://github.com/michaelroth1/mitoSoft.DailyTimers.Core</PackageProjectUrl>
    <PackageIcon>mitoSoft.png</PackageIcon>
    <PackageTags>timeswitch time trigger timer daily-timer weekly-timer</PackageTags>
    <AssemblyVersion></AssemblyVersion>
    <Version>1.1.3</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="mitoSoft.Holidays" Version="1.1.3" />
  </ItemGroup>

  <ItemGroup>
    <None Update="mitoSoft.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
